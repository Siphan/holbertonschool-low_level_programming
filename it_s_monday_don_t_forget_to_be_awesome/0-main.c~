#include <stdio.h>
char lowercase(char);

int main(void)
{
  char c;

  c = lowercase('a');
  printf("%c\n", c);
  c = lowercase('h');
  printf("%c\n", c);
  c = lowercase('A');
  printf("%c\n", c);
  c = lowercase('{');
  printf("%c\n", c);
  return (0);
}
ubuntu@ip-172-31-63-244:~/holbertonschool$ gcc -Wall -Werror -Wextra -pedantic 0-main.c 0-lowercase.c -o lowercase && ./lowercase
a
h
a
{
